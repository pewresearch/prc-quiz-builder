{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "prc-quiz/group-results",
	"version": "0.1.0",
	"title": "Group Results",
  "description": "Results for a community group as given by group id and archetype hash id",
	"category": "text",
	"attributes": {
		"allowedBlocks": {
			"type": "array"
		},
		"name": {
			"type": "string"
		},
		"total": {
			"type": "integer"
		},
		"typologyGroups": {
			"type": "string"
		},
		"answers": {
			"type": "string"
		}
	},
	"supports": {
		"anchor": true,
		"html": false,
		"multiple": false,
		"color": {
			"background": true,
			"text": true,
			"link": true
		},
		"spacing": {
			"blockGap": true,
			"margin": [ "top", "bottom" ],
			"padding": true
		},
		"typography": {
			"fontSize": true,
			"__experimentalFontFamily": true
		}
	},
	"parent": [ "prc-quiz/controller" ],
	"providesContext": {
		"prc-quiz/groups/results/name": "name",
		"prc-quiz/groups/results/typologyGroups": "typologyGroups",
		"prc-quiz/groups/results/answers": "answers",
		"prc-quiz/groups/results/total": "total"
	},
	"textdomain": "group-results",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php"
}
