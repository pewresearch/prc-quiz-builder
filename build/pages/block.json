{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "prc-quiz/pages",
  "version": "4.0",
  "title": "Pages",
  "category": "quiz",
  "allowedBlocks": [
    "prc-quiz/page"
  ],
  "attributes": {},
  "supports": {
    "anchor": false,
    "html": false,
    "multiple": false,
    "interactivity": true,
    "color": {
      "background": true,
      "text": true,
      "link": true,
      "button": true
    },
    "layout": {
      "default": {
        "type": "constrained"
      },
      "allowSwitching": true,
      "allowInheriting": true,
      "allowVerticalAlignment": true,
      "allowJustification": true,
      "allowOrientation": false,
      "allowSizingOnChildren": true
    },
    "typography": {
      "fontSize": true,
      "__experimentalFontFamily": true
    }
  },
  "parent": [
    "prc-quiz/controller"
  ],
  "textdomain": "pages",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScriptModule": "file:./view.js"
}